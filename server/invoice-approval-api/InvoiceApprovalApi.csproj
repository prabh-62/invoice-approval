    <PlatformTarget>x64</PlatformTarget>
    <Nullable>enable</Nullable>
    <Features>strict</Features>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="5.6.3" />
